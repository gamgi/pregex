
Regex 			= _{ SOI ~ Alternation ~ EOI }
Alternation     =  { ( ConcatMaybe | Concat | Item ) ~ ( "|" ~ Alternation )? }
ConcatMaybe     =  { ( Concat | Item ) ~ (ConcatMaybe)? }
Concat          =  { Item ~ Item }
Item 			= _{ Quantified | Literal }
Quantified      =  { Literal ~ Quantifier }
Literal 		=  { ASCII_ALPHA | " " }
Quantifier	    =  { "+" | "?" | "*" }